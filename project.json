{
    "platforms": {
        "darwin": {
            "aspects": [
                "meta",
                "dotfiles",
                "homebrew",
                "karabiner",
                "launchd",
                "terminfo",
                "vim",
                "cron",
                "defaults"
            ],
            "variables": {}
        },
        "linux": {
            "aspects": ["meta", "dotfiles", "terminfo", "vim"],
            "variables": {
                "terminfoPath": "~/share/terminfo"
            }
        }
    },
    "profiles": {
        "personal": {
            "pattern": "/\\bretiro\\b/i",
            "variables": {
                "corpusNotes": "~/Sync/Personal/Corpus"
            }
        },
        "work": {
            "pattern": "/\\blfrw044\\b/i",
            "variables": {}
        }
    },
    "variables": {
        "corpusNotes": "~/Documents/Corpus",
        "figManaged": "vim: set nomodifiable : DO NOT EDIT - edit template source instead («file») or use `:set modifiable` to force.",
        "gitCipherPath": "vendor/git-cipher/bin/git-cipher"
    }
}
