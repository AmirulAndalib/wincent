    "variables": {
        "corpusNotes": "~/Documents/Corpus",
        "figManaged": "vim: set nomodifiable : DO NOT EDIT - edit template source instead («file») or use `:set modifiable` to force.",
        "gitCipherPath": "vendor/git-cipher/bin/git-cipher",
        "iTermDynamicProfiles": {
            "external": [
                {
                    "path": "Mutt.json",
                    "src": "40-Mutt-4K.json"
                },
                {
                    "path": "Vim.json",
                    "src": "70-Vim-4K.json"
