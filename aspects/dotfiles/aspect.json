            ".gitk",
            ".hammerspoon",
            ".hgrc",
            ".imapfilter",
            ".inputrc",
            ".lbdbrc",
            ".mutt",
            ".rdebugrc",
            ".rgrc",
            ".rspec",
            ".screenrc",
            ".tmux.conf",
            ".urlview",
            ".zsh",
            ".zshenv",
            ".zshrc"
