{
    "compilerOptions": {
        "module": "esnext",
        "moduleResolution": "node",
        "noFallthroughCasesInSwitch": true,
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "outDir": "./lib",
        "strict": true,
        "target": "ES2019"
    },
    "include": ["aspects/**/*.ts", "src/**/*.ts"]
}
